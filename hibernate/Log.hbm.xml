<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="src.main.java.com.llc.model.Log">
        <id name="lid">
            <generator class="uuid"/>
        </id>
        <property name="content"/>
        <property name="time"/>
        <property name="ip"/>
        <property name="log_level"/>
        <property name="member_id"/>
        <property name="device_id"/>
        <property name="modtrans"/>
    </class>
</hibernate-mapping>